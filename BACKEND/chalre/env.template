# Copy this file to .env or set these as system environment variables
# Database Configuration
DB_URL=jdbc:mysql://localhost:3306/chalre
DB_USERNAME=root
DB_PASSWORD=your_database_password

# JWT Secret Key (should be at least 256 bits / 32 characters for HS256)
JWT_SECRET=your_jwt_secret_key_here_minimum_32_characters_long

# Razorpay Payment Gateway
RAZORPAY_KEY=your_razorpay_key
RAZORPAY_SECRET=your_razorpay_secret
RAZORPAY_ACCOUNT_NUMBER=your_razorpay_account_number

# Cloudinary Image Upload Service
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret
